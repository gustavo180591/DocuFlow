{
  "name": "docuflow",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf node_modules .svelte-kit .vite dist .turbo",
    "typegen": "svelte-kit sync",
    "dev:clean": "npm run clean && npm i && npm run typegen && npm run dev",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "ocr:check": "node --loader ts-node/esm scripts/ocr-check.ts",
    "test:ocr": "npm run ocr:check test/files/sample.pdf",
    "test:extract": "node --loader ts-node/esm scripts/test-extract.mjs",
    "test:text-extract": "node scripts/test-text-extract.mjs"
  },
  "dependencies": {
    "@prisma/client": "^5.0.0",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@types/pdfjs-dist": "^2.10.377",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^5.4.149",
    "pdfkit": "^0.17.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "1.30.4",
    "@sveltejs/vite-plugin-svelte": "2.5.3",
    "@tailwindcss/forms": "^0.5.10",
    "@types/node": "^20.11.0",
    "@types/pdf-parse": "^1.1.5",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.31",
    "prettier": "^3.2.4",
    "prettier-plugin-svelte": "^3.1.2",
    "prisma": "^5.10.0",
    "rimraf": "^5.0.5",
    "svelte": "4.2.19",
    "svelte-check": "^3.6.5",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "4.5.2",
    "zod": "^3.25.76"
  }
}
